{
  "name": "barterapp",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": ">= 7.0.0",
    "npm": "6.13.4"
  },
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "build-client": "ng build",
    "build-client-watch": "ng build --watch",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "heroku-postbuild": "ng build --prod",
    "postinstall": "touch secrets.js",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md,ts,tsx}\"",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev":
      "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server":
      "nodemon server -e html,js,scss --ignore public --ignore client",
    "test":
      "NODE_ENV='test' ng test && mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "ng-e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["prettier --write", "git add"],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "~8.3.24",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@google/maps": "^1.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.2.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^6.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.1",
    "history": "^4.9.0",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "rxjs": "~6.4.0",
    "sequelize": "^5.21.3",
    "socket.io": "^2.2.0",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular/cli": "~8.3.24",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "axios-mock-adapter": "^1.16.0",
    "chai": "^4.2.0",
    "codelyzer": "^5.0.0",
    "enhanced-resolve": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "git-url-parse": "^11.1.2",
    "husky": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.3",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "protractor": "~5.4.0",
    "simple-git": "^1.121.0",
    "supertest": "^4.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "webpack": "^4.41.5",
    "yaml": "^1.6.0"
  }
}
